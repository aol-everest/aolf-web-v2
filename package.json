{
  "name": "aolf-web",
  "version": "0.1.0",
  "private": true,
  "cacheDirectories": [
    ".next/cache"
  ],
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "develop": "cross-env NODE_ENV=development yarn local-run",
    "develop:qa": "cross-env NODE_ENV=test yarn local-run",
    "develop:prod": "cross-env NODE_ENV=production yarn local-run",
    "debug:qa": "cross-env NODE_ENV=qa NODE_OPTIONS='--inspect' yarn local-run",
    "local-run": "next dev",
    "build": "next build",
    "start": "next start",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "lint": "next lint",
    "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon public/icons",
    "eslint": "./node_modules/.bin/eslint --fix \"./**/*.{js,jsx}\"",
    "prettify": "./node_modules/.bin/prettier --write \"./**/*.{js,jsx,json,css,scss,md,yml,yaml}\"",
    "prepare": "husky",
    "commit": "cz",
    "check:format": "prettier --check '**/*.{js,jsx,json,css,scss,md,yml,yaml}'",
    "check:lint": "eslint . --ext .js,.jsx --color",
    "check:spelling": "cspell \"{README.md,TODO.md,.github/*.md,src/**/*.js,src/**/*.jsx}\"",
    "fix:format": "prettier --check --write --ignore-unknown .",
    "fix:lint": "npm run check:lint -- --fix",
    "check:staged": "lint-staged",
    "update-deps": "npm-check-updates -u",
    "kill": "kill-port 3000",
    "heroku-cleanup": "rm -rf .next/cache && find node_modules/@next -depth -maxdepth 1 -type d -name 'swc*' -exec rm -rf {} \\; -prune #Remove cache and SWC binaries to reduce heroku slug size",
    "coupling-graph": "npx madge --extensions js,jsx,ts,tsx,css,md,scss ./ --exclude '.next|tailwind.config.js|reset.d.ts|prettier.config.js|postcss.config.js|playwright.config.ts|next.config.js|next-env.d.ts|instrumentation.ts|e2e/|README.md|.storybook/|.eslintrc.js' --image graph.svg"
  },
  "dependencies": {
    "@analytics/router-utils": "^0.1.1",
    "@analytics/segment": "^2.1.0",
    "@contentful/rich-text-react-renderer": "^15.22.11",
    "@emotion/react": "^11.14.0",
    "@hookform/devtools": "^4.3.3",
    "@hookform/resolvers": "^3.10.0",
    "@microlink/react-json-view": "^1.26.1",
    "@next/bundle-analyzer": "^14.2.3",
    "@paypal/react-paypal-js": "^8.8.2",
    "@popperjs/core": "^2.11.8",
    "@sentry/nextjs": "^8.55.0",
    "@stripe/react-stripe-js": "^2.7.3",
    "@stripe/stripe-js": "^3.4.0",
    "@tanstack/react-query": "^5.66.7",
    "@tanstack/react-query-devtools": "^5.66.7",
    "@uidotdev/usehooks": "^2.4.1",
    "amazon-cognito-auth-js": "^1.3.3",
    "amazon-cognito-identity-js": "^6.3.12",
    "amazon-cognito-passwordless-auth": "^0.13.2",
    "analytics": "^0.8.16",
    "append-query": "^2.1.1",
    "aws-amplify": "^6.13.0",
    "axios": "^1.7.9",
    "bootstrap": "^4.6.2",
    "bootstrap-daterangepicker": "^3.1.0",
    "classnames": "^2.5.1",
    "contentful": "^11.5.0",
    "cookies-next": "^4.3.0",
    "dayjs": "^1.11.13",
    "embla-carousel-react": "^8.1.3",
    "formik": "^2.4.6",
    "formik-wizard-form": "^2.1.0",
    "framer-motion": "^11.18.2",
    "helmet": "^7.1.0",
    "heroku-ssl-redirect": "^0.1.1",
    "is-url": "^1.2.4",
    "js-cookie": "^3.0.5",
    "moment-timezone": "^0.5.47",
    "next": "^14.2.3",
    "next-compose-plugins": "^2.2.1",
    "next-seo": "^6.6.0",
    "nprogress": "^0.2.0",
    "nuqs": "^2.4.0",
    "query-string": "^9.1.1",
    "react": "^18.2.0",
    "react-awesome-reveal": "^4.3.1",
    "react-bootstrap": "1.6.7",
    "react-content-loader": "^7.0.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-flatpickr": "^3.10.13",
    "react-google-autocomplete": "^2.7.4",
    "react-google-maps": "^9.4.5",
    "react-highlight-words": "^0.21.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "react-idle-timer": "^5.7.2",
    "react-image-crop": "^11.0.7",
