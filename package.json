    "react-scroll": "^1.9.3",
    "react-scroll-percentage": "^4.3.2",
    "react-select": "^5.10.0",
    "react-select2-wrapper": "^1.0.4-beta6",
    "react-slick": "^0.29.0",
    "react-social-media-embed": "^2.5.18",
    "react-tag-input": "^6.10.6",
    "react-text-mask": "^5.5.0",
    "react-tooltip": "^5.28.0",
    "react-uid": "^2.4.0",
    "react-use": "^17.6.0",
    "react-youtube": "^10.1.0",
    "reactstrap": "^8.10.1",
    "remove-markdown": "^0.6.0",
    "rsuite": "^5.61.0",
    "sass": "^1.77.2",
    "select2": "^4.1.0-rc.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^9.4.1",
    "tldts": "^6.1.77",
    "use-analytics": "^1.1.0",
    "video-react": "^0.16.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@svgr/cli": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.35.6",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "cspell": "^8.9.1",
    "cssnano": "^7.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "kill-port": "^2.0.1",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --quiet --fix",
      "prettier --write"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss,html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
