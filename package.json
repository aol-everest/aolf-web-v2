{
  "name": "aolf-web",
  "version": "0.1.0",
  "private": true,
  "cacheDirectories": [
    ".next/cache"
  ],
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "develop": "NODE_ENV=development next dev",
    "develop:qa": "NODE_ENV=test next dev",
    "develop:prod": "NODE_ENV=production next dev",
    "build": "next build",
    "start": "node server.js",
    "build:analyze": "ANALYZE=true npm run build",
    "lint": "next lint",
    "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon public/icons",
    "eslint": "./node_modules/.bin/eslint --fix \"./**/*.{js,jsx}\"",
    "prettify": "./node_modules/.bin/prettier --write \"./**/*.{js,jsx,json,css,scss}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@builder.io/react": "^2.0.10",
    "@builder.io/widgets": "^1.2.23",
    "@contentful/rich-text-react-renderer": "^15.12.1",
    "@googlemaps/js-api-loader": "^1.14.3",
    "@hookform/devtools": "^4.2.2",
    "@hookform/resolvers": "^2.9.9",
    "@next/bundle-analyzer": "^12.3.1",
    "@paypal/react-paypal-js": "^7.8.1",
    "@phntms/next-gtm": "^0.1.0",
    "@popperjs/core": "^2.11.6",
    "@segment/snippet": "^4.15.3",
    "@sentry/nextjs": "^7.15.0",
    "@stripe/react-stripe-js": "^1.13.0",
    "@stripe/stripe-js": "^1.41.0",
    "amazon-cognito-auth-js": "^1.3.3",
    "amazon-cognito-identity-js": "^5.2.11",
    "axios": "^1.1.3",
    "bootstrap": "^4.5.3",
    "bootstrap-daterangepicker": "^3.1.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.5",
    "embla-carousel": "^4.5.3",
    "express": "^4.18.2",
    "formik": "^2.2.9",
    "gsap": "^3.11.3",
    "helmet": "^6.0.0",
    "heroku-ssl-redirect": "^0.1.1",
    "next": "^12.3.1",
    "next-compose-plugins": "^2.2.1",
    "next-favicon-loader": "^3.0.5",
    "next-seo": "^5.6.0",
    "nocache": "^3.0.4",
    "nprogress": "^0.2.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-bootstrap": "1.6.4",
    "react-content-loader": "^6.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.37.0",
    "react-html-parser": "^2.0.2",
    "react-icons": "^4.6.0",
    "react-image-crop": "^9.1.1",
    "react-lines-ellipsis": "^0.15.3",
    "react-number-format": "^5.0.1",
    "react-places-autocomplete": "^7.3.0",
    "react-popper": "^2.3.0",
    "react-query": "^3.39.2",
    "react-reveal": "^1.2.2",
    "react-scroll": "^1.8.7",
    "react-select": "^5.5.0",
    "react-text-mask": "^5.5.0",
    "react-tooltip": "^4.4.0",
    "react-uid": "^2.3.2",
    "react-use": "^17.4.0",
    "reactstrap": "^8.10.1",
    "remove-markdown": "^0.5.0",
    "sass": "^1.55.0",
    "swiper": "^7.4.1",
    "video-react": "^0.15.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@svgr/cli": "^6.5.0",
    "autoprefixer": "^10.4.12",
    "cssnano": "^5.1.13",
    "eslint": "^8.25.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --quiet --fix",
      "prettier --write"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
