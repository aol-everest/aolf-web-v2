{
  "name": "aolf-web",
  "version": "0.1.0",
  "private": true,
  "cacheDirectories": [
    ".next/cache"
  ],
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "develop": "cross-env NODE_ENV=development yarn local-run",
    "develop:qa": "cross-env NODE_ENV=test yarn local-run",
    "develop:prod": "cross-env NODE_ENV=production yarn local-run",
    "debug:qa": "cross-env NODE_ENV=qa NODE_OPTIONS='--inspect' yarn local-run",
    "local-run": "next dev",
    "build": "next build",
    "start": "node server.js",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "lint": "next lint",
    "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon public/icons",
    "eslint": "./node_modules/.bin/eslint --fix \"./**/*.{js,jsx}\"",
    "prettify": "./node_modules/.bin/prettier --write \"./**/*.{js,jsx,json,css,scss,md,yml,yaml}\"",
    "prepare": "is-ci || husky install",
    "commit": "cz",
    "check:format": "prettier --check '**/*.{js,jsx,json,css,scss,md,yml,yaml}'",
    "check:lint": "eslint . --ext .js,.jsx --color",
    "check:spelling": "cspell \"{README.md,TODO.md,.github/*.md,src/**/*.js,src/**/*.jsx}\"",
    "fix:format": "prettier --check --write --ignore-unknown .",
    "fix:lint": "npm run check:lint -- --fix",
    "check:staged": "lint-staged",
    "update-deps": "npm-check-updates -u",
    "kill": "kill-port 3000"
  },
  "dependencies": {
    "@analytics/router-utils": "^0.1.1",
    "@analytics/segment": "^1.1.4",
    "@contentful/rich-text-react-renderer": "^15.19.6",
    "@hookform/devtools": "^4.3.1",
    "@hookform/resolvers": "^3.3.4",
    "@next/bundle-analyzer": "^14.1.3",
    "@paypal/react-paypal-js": "^8.1.4",
    "@popperjs/core": "^2.11.8",
    "@sentry/nextjs": "^7.106.1",
    "@stripe/react-stripe-js": "^2.6.0",
    "@stripe/stripe-js": "^2.4.0",
    "@tanstack/react-query": "^5.28.4",
    "@tanstack/react-query-devtools": "^5.28.4",
    "@uidotdev/usehooks": "^2.4.1",
    "amazon-cognito-auth-js": "^1.3.3",
    "amazon-cognito-identity-js": "^6.3.12",
    "analytics": "^0.8.11",
    "append-query": "^2.1.1",
    "axios": "^1.5.0",
    "bootstrap": "^4.6.2",
    "bootstrap-daterangepicker": "^3.1.0",
    "classnames": "^2.5.1",
    "cookies-next": "^4.1.1",
    "dayjs": "^1.11.10",
    "embla-carousel": "^4.5.3",
    "express": "^4.18.3",
    "formik": "^2.4.5",
    "formik-wizard-form": "^2.1.0",
    "gsap": "^3.12.5",
    "helmet": "^7.1.0",
    "heroku-ssl-redirect": "^0.1.1",
    "is-url": "^1.2.4",
    "moment-timezone": "^0.5.45",
    "next": "^14.1.3",
    "next-compose-plugins": "^2.2.1",
    "next-seo": "^6.5.0",
    "nprogress": "^0.2.0",
    "nuqs": "^1.17.1",
    "query-string": "^9.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "1.6.7",
    "react-content-loader": "^7.0.0",
    "react-dnd": "^14.0.5",
    "react-dnd-html5-backend": "^14.1.0",
    "react-dom": "^18.2.0",
    "react-flatpickr": "^3.10.13",
    "react-google-maps": "^9.4.5",
    "react-highlight-words": "^0.20.0",
    "react-hook-form": "^7.51.0",
    "react-icons": "^4.12.0",
    "react-image-crop": "^11.0.5",
    "react-intersection-observer": "^9.8.1",
    "react-lines-ellipsis": "^0.15.4",
    "react-phone-input-2": "^2.15.1",
    "react-places-autocomplete": "^7.3.0",
    "react-player": "^2.15.1",
    "react-popper": "^2.3.0",
    "react-reveal": "^1.2.2",
    "react-scroll": "^1.9.0",
    "react-select": "^5.8.0",
    "react-select2-wrapper": "^1.0.4-beta6",
    "react-slick": "^0.29.0",
    "react-tag-input": "^6.9.0",
    "react-text-mask": "^5.5.0",
    "react-tooltip": "^5.26.3",
    "react-uid": "^2.3.3",
    "react-use": "^17.5.0",
    "react-youtube": "^10.1.0",
    "reactstrap": "^8.10.1",
    "remove-markdown": "^0.5.0",
    "sass": "^1.71.1",
    "select2": "^4.1.0-rc.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^9.4.1",
    "use-analytics": "^1.1.0",
    "video-react": "^0.16.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@svgr/cli": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.27.7",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "cspell": "^8.6.0",
    "cssnano": "^6.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "kill-port": "^2.0.1",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --quiet --fix",
      "prettier --write"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss,html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
