{
  "name": "aolf-web",
  "version": "0.1.0",
  "private": true,
  "cacheDirectories": [
    ".next/cache"
  ],
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "develop": "cross-env NODE_ENV=development yarn local-run",
    "develop:qa": "cross-env NODE_ENV=test yarn local-run",
    "develop:prod": "cross-env NODE_ENV=production local-run",
    "debug:qa": "cross-env NODE_ENV=qa NODE_OPTIONS='--inspect' yarn local-run",
    "local-run": "next dev",
    "build": "next build",
    "start": "node server.js",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "lint": "next lint",
    "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon public/icons",
    "eslint": "./node_modules/.bin/eslint --fix \"./**/*.{js,jsx}\"",
    "prettify": "./node_modules/.bin/prettier --write \"./**/*.{js,jsx,json,css,scss}\"",
    "prepare": "husky install",
    "commit": "cz",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --ext .js,.jsx --color",
    "check:spelling": "cspell \"{README.md,TODO.md,.github/*.md,src/**/*.js,src/**/*.jsx}\"",
    "fix:format": "prettier --check --write --ignore-unknown .",
    "fix:lint": "npm run check:lint -- --fix",
    "check:staged": "lint-staged",
    "update-deps": "npm-check-updates -u",
    "kill": "kill-port 3000"
  },
  "dependencies": {
    "@analytics/google-tag-manager": "^0.5.3",
    "@analytics/router-utils": "^0.1.1",
    "@analytics/segment": "^1.1.4",
    "@aws-amplify/analytics": "^6.0.9",
    "@contentful/rich-text-react-renderer": "^15.16.2",
    "@googlemaps/js-api-loader": "^1.15.1",
    "@hookform/devtools": "^4.3.0",
    "@hookform/resolvers": "^2.9.10",
    "@next/bundle-analyzer": "^13.0.6",
    "@paypal/react-paypal-js": "^8.1.0",
    "@popperjs/core": "^2.11.6",
    "@segment/snippet": "^4.15.3",
    "@sentry/nextjs": "^7.59.2",
    "@stripe/react-stripe-js": "^2.1.1",
    "@stripe/stripe-js": "^1.54.2",
    "amazon-cognito-auth-js": "^1.3.3",
    "amazon-cognito-identity-js": "^5.2.11",
    "analytics": "^0.8.1",
    "append-query": "^2.1.1",
    "axios": "^1.2.1",
    "bootstrap": "^4.5.3",
    "bootstrap-daterangepicker": "^3.1.0",
    "classnames": "^2.3.2",
    "cookies-next": "^2.1.2",
    "dayjs": "^1.11.7",
    "embla-carousel": "^4.5.3",
    "express": "^4.18.2",
    "formik": "^2.2.9",
    "formik-wizard-form": "^2.1.0",
    "gsap": "^3.11.3",
    "helmet": "^7.0.0",
    "heroku-ssl-redirect": "^0.1.1",
    "next": "^13.4.10",
    "next-compose-plugins": "^2.2.1",
    "next-favicon-loader": "^3.0.5",
    "next-seo": "^6.1.0",
    "nocache": "^3.0.4",
    "nprogress": "^0.2.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-bootstrap": "1.6.4",
    "react-content-loader": "^6.2.0",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "^18.2.0",
    "react-flatpickr": "^3.10.13",
    "react-hook-form": "^7.40.0",
    "react-html-parser": "^2.0.2",
    "react-icons": "^4.7.1",
    "react-image-crop": "^10.0.9",
    "react-lines-ellipsis": "^0.15.3",
    "react-phone-input-2": "^2.15.1",
    "react-places-autocomplete": "^7.3.0",
    "react-popper": "^2.3.0",
    "react-query": "^3.39.2",
    "react-reveal": "^1.2.2",
    "react-scroll": "^1.8.8",
    "react-select": "^5.7.0",
    "react-select2-wrapper": "^1.0.4-beta6",
    "react-tag-input": "^6.8.1",
    "react-text-mask": "^5.5.0",
    "react-tooltip": "^5.0.0",
    "react-uid": "^2.3.2",
    "react-use": "^17.4.0",
    "reactstrap": "^8.10.1",
    "remove-markdown": "^0.5.0",
    "sass": "^1.56.1",
    "swiper": "^9.1.0",
    "use-analytics": "^1.1.0",
    "video-react": "^0.16.0",
    "yup": "^0.32.11",
    "yup-phone": "^1.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@svgr/cli": "^8.0.1",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.29.0",
    "eslint-config-next": "^13.4.12",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "kill-port": "^2.0.1",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.19",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tailwindcss": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --quiet --fix",
      "prettier --write"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
