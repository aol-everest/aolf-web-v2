{
  "name": "aolf-web",
  "version": "0.1.0",
  "private": true,
  "cacheDirectories": [
    ".next/cache"
  ],
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "develop": "NODE_ENV=development next dev",
    "develop:qa": "NODE_ENV=test next dev",
    "develop:prod": "NODE_ENV=production next dev",
    "build": "next build",
    "start": "node server.js",
    "build:analyze": "ANALYZE=true npm run build",
    "lint": "next lint",
    "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon public/icons",
    "eslint": "./node_modules/.bin/eslint --fix \"./**/*.{js,jsx}\"",
    "prettify": "./node_modules/.bin/prettier --write \"./**/*.{js,jsx,json,css,scss}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@builder.io/react": "^1.1.49",
    "@builder.io/widgets": "^1.2.21",
    "@contentful/rich-text-react-renderer": "^15.11.1",
    "@googlemaps/js-api-loader": "^1.13.7",
    "@hookform/devtools": "^4.0.2",
    "@hookform/resolvers": "^2.8.8",
    "@next/bundle-analyzer": "^12.1.0",
    "@phntms/next-gtm": "^0.1.0",
    "@popperjs/core": "^2.11.2",
    "@segment/snippet": "^4.15.3",
    "@sentry/nextjs": "^6.18.1",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.23.0",
    "aws-amplify": "^4.3.15",
    "axios": "^0.26.0",
    "bootstrap": "^4.5.3",
    "bootstrap-daterangepicker": "^3.1.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.8",
    "embla-carousel": "^4.5.3",
    "express": "^4.17.3",
    "formik": "^2.2.9",
    "gsap": "^3.9.1",
    "heroku-ssl-redirect": "^0.1.1",
    "next": "^12.1.0",
    "next-compose-plugins": "^2.2.1",
    "next-favicon-loader": "^3.0.5",
    "next-pwa": "^5.4.4",
    "next-seo": "^5.1.0",
    "nprogress": "^0.2.0",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-bootstrap": "1.6.4",
    "react-content-loader": "^6.1.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.1",
    "react-html-parser": "^2.0.2",
    "react-icons": "^4.3.1",
    "react-image-crop": "^9.1.1",
    "react-lines-ellipsis": "^0.15.0",
    "react-number-format": "^4.9.1",
    "react-paypal-button-v2": "^2.6.3",
    "react-places-autocomplete": "^7.3.0",
    "react-popper": "^2.2.5",
    "react-query": "^3.34.16",
    "react-reveal": "^1.2.2",
    "react-scroll": "^1.8.5",
    "react-select": "^5.2.2",
    "react-text-mask": "^5.4.3",
    "react-tooltip": "^4.2.21",
    "react-uid": "^2.3.1",
    "react-use": "^17.3.2",
    "reactstrap": "^8.10.1",
    "remove-markdown": "^0.3.0",
    "sass": "^1.49.9",
    "swiper": "^7.4.1",
    "video-react": "^0.15.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@svgr/cli": "^6.2.1",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.17",
    "eslint": "^8.6.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --quiet --fix",
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
